<!DOCTYPE html>
<html>
<head>
    <title>Edit AUDITORIUM</title>
</head>
<body>
<body style="background-color:powderblue;">

<h1>Edit AUDITORIUM</h1>

<form action="/auditorium/update" method="get">
    <input type="hidden"
           id="AUDITORIUM_ID"
           name="AUDITORIUM_ID"
           value="<%= auditorium.AUDITORIUM_ID %>"
    />

    AUDITORIUM CAPACITY: <input name ="CAPACITY" type="text" /><br/>
<br>
    FK CINEMA_ID: <br>
    <select id="CINEMA_ID" name="CINEMA_ID">
        <%for(var i = 0; cinema.length > i; i++) { %>
        <option value="<%= cinema[i].CINEMA_ID %>"> <%= cinema[i].CINEMA_NAME %> </option>
        <% } %>
    </select>
    <br>
<br>
    FK MOVIE_ID: <br> <select id="MOVIE_ID" name="MOVIE_ID">
        <%for(var i = 0; movie.length > i; i++) { %>
        <option value="<%= movie[i].MOVIE_ID %>"> <%= movie[i].MOVIE_NAME %> </option>
        <% } %>
    </select>


    <br/>



    <br />


    <br />

    <input type="submit" />

</form>
</body>
</html>